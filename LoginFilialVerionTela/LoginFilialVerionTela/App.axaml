<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:LoginFilialVerionTela"
			 xmlns:controls="clr-namespace:LoginFilialVerionTela"
             x:Class="LoginFilialVerionTela.App"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <FluentTheme />
		<StyleInclude Source="avares://LoginFilialVerionTela/Assets/Icons.axaml"/>
		<StyleInclude Source="avares://LoginFilialVerionTela/CardButtonControl.axaml" />
    </Application.Styles>
</Application>